<% @users.each do |user| %>
  <hr>
  <%= "username: #{user.username}" %>
  <%= "| email: #{user.email}" %>
  <%= "| account created #{time_ago_in_words user.created_at} ago." %>
  <%= link_to "reset password", admin_user_path(user, user: {password: 'password', password_confirmation: 'password'}), method: :put, data: {confirm: "are you sure?"}, class: 'btn' %>
<% end %>