<% @notifications.each do |n| %>
  <%= render "shared/popups/#{n}" %>
<% end %>

<% if notice %>
  <p class="alert container">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= notice %>
  </p>
<% end %>
<% if alert %>
  <p class="alert container">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= alert %>
  </p>
<% end %>